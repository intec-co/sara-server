{
	"name": "sara-skel-server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "npm run build:live",
		"test": "echo \"Error: no test specified\" && exit 1",
		"watch-node": "nodemon dist/server.js",
		"watch-ts": "tsc -w",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
		"build:live": "npm run copy:files; nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts dist/conf/",
		"copy:files": "cp package.json dist/; cp src/conf/key dist/conf;cd src;find ./ -name '*.json' -type f -exec cp --parents {} ../dist/ \\;"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/intec-co/sara-skel-server.git"
	},
	"author": "cavargasp@intec.co",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/intec-co/sara-skel-server/issues"
	},
	"homepage": "https://github.com/intec-co/sara-skel-server#readme",
	"devDependencies": {
		"@types/mongodb": "^3.5.8",
		"@types/node": "^13.13.0",
		"concurrently": "^5.1.0",
		"jest": "^25.3.0",
		"nodemon": "^2.0.3",
		"ts-jest": "^25.4.0",
		"ts-node": "^8.8.2",
		"tslint": "^6.1.1",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"class-transformer": "^0.2.3",
		"class-transformer-validator": "^0.8.0",
		"class-validator": "^0.12.1",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"mongodb": "^3.5.6",
		"ws": "^7.2.3"
	},
	"_moduleAliases": {
		"@Interfaces": "dist/interfaces",
		"@Services": "dist/services",
		"@Lib": "dist/lib"
	}
}
